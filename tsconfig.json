{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "types": []
  },
  "include": [
    "**/*.js",
    "**/*.jsx",
    "src/app/components/Featured.jsx",
    "src/app/components/Finished.jsx",
    "src/app/components/HeroSection.jsx",
    "src/app/components/NavBar.jsx",
    "src/app/components/Newsletter.jsx",
    "src/app/components/Partner.jsx",
    "src/app/components/Value.jsx",
    "src/app/layout.jsx",
    "src/app/page.jsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
